{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": "warn",
        "no-plusplus": "off",
        "import/no-dynamic-require": "off",
        "no-throw-literal": "off",
        "no-await-in-loop": "off",
        "no-tabs": "off"
    }
}
